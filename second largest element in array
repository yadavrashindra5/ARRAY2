#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"enter the number from keyboard"<<"\n";
    cin>>n;
    int arr[n];
    int j;
    for(j=0;j<n;j++)
    {
        cin>>arr[j];
    }
    int i,arr1,largest;
    arr1=arr[0];
    largest=arr[2];
    for(i=1;i<5;i++)
    {
        if(arr1>arr[i])
        { if(arr[i]>largest)
            largest=arr[i];
        }
            else
        {largest=arr1;
        arr1=arr[i];

        }
    }
    cout<<largest<<"\n";

}
